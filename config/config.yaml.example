hosts: /etc/hosts
ssh_config: /home/your_name/.ssh/config

servers:
  # production server
  - name: production-server
    address: 1.1.1.1
    user: root
    identity_file: ~/.ssh/private_key
  # staging server
  - name: staging-server
    address: 2.2.2.2
    user: root
    identity_file: ~/.ssh/private_key